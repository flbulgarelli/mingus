grammar org.uqbar.mingus.Mingus with org.eclipse.xtext.common.Terminals

generate mingus "http://www.uqbar-project.org/mingus/Mingus"

Program:
	value=Term;

Term returns Term:
	Term0;

Term0 returns Term:
	Term1 | Abstraction;

Term1 returns Term:
	Term2 ({Application.function=current} argument=Term2)*;

Term2 returns Term:
	Term3 | Variable;

Term3 returns Term:
	'(' Term0 ')';

Abstraction:
	'\\' parameter=ID '.' body=Term0;

Variable:
	name=ID;	







 

